{
  "version": 2,
  "buildCommand": "echo 'Static site'",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/scripts/(.*)",
      "dest": "/scripts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
